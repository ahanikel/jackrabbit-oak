cd $(dirname $0)
export KAFKA_TOPIC=nodestates
export KAFKA_SERVERS=localhost:9092
ps auxww | grep java | grep oak-store-zeromq-service-store | awk '{print $2}' | xargs kill -9
sleep 5
java -Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=6006 -jar $(ls -1 target/*.jar | grep jar-with)


